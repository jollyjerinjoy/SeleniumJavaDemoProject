<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="CrossBrowserTest">
  <test name="Chrometest">
    <classes>
       <class name="testscript.LoginTest">
       <parameter name="browser" value="chrome"></parameter>
		<methods>
		<!--<include name="verifyloginwithvalidcredentails"></include>
		<include name="verifylogininvalidpassword"></include>-->
		<exclude name="verifyloginwithvalidcredentails"></exclude>
		<exclude name="verifylogininvalidpassword"></exclude>
		</methods>
		</class>
    </classes>
  </test> <!-- Test -->
    <test name="FirefoxTest">
    <classes>
       <class name="testscript.LoginTest">
       <parameter name="browser" value="edge"></parameter>
		<methods>
		<!--<include name="verifyloginwithvalidcredentails"></include>
		<include name="verifylogininvalidpassword"></include>-->
		<exclude name="verifyloginwithvalidcredentails"></exclude>
		<exclude name="verifylogininvalidpassword"></exclude>
		</methods>
		</class>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
